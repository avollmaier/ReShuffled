
(rules PCB Atmega324P-Diplomarbeit-Perl-neu
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 3204)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.2)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.8)
    )
  )
  (rule
    (width 250.0)
    (clear 200.2)
    (clear 125.0 (type smd_to_turn_gap))
    (clear 50.0 (type smd_smd))
  )
  (padstack "Via[0-1]_600:400_um"
    (shape
      (circle F.Cu 600.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 600.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_600:400_um" "Via[0-1]_600:400_um" default
  )
  (via 
    "Via[0-1]_600:400_um-kicad_default" "Via[0-1]_600:400_um" "kicad_default"
  )
  (via 
    "Via[0-1]_600:400_um-5V" "Via[0-1]_600:400_um" 5V
  )
  (via_rule
    default "Via[0-1]_600:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_600:400_um-kicad_default"
  )
  (via_rule
    5V "Via[0-1]_600:400_um-5V"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    12V GND "Net-(C3-Pad1)" "Net-(C4-Pad1)" "Net-(C6-Pad1)" +3V3 /Peripherie/12V "Net-(C12-Pad1)"
    "Net-(C13-Pad2)" MISO SCK MOSI +5V "Net-(D3-Pad2)" "Net-(D4-Pad2)" S1
    S2 S3 "Net-(J1-Pad2)" "RESET_VCC" /Peripherie/TxD /Peripherie/RxD EN SLEEP
    STEP DIR "Net-(J4-Pad2)" "Net-(J4-Pad5)" "Net-(J4-Pad6)" "Net-(J7-Pad2)" "Net-(J7-Pad4)" "Net-(J7-Pad3)"
    "Net-(L1-Pad1)" "Net-(Q1-Pad2)" "Net-(Q1-Pad1)" "Net-(Q2-Pad1)" "Net-(Q3-Pad2)" "Net-(Q4-Pad2)" M1 M2
    "Net-(U1-Pad4)" "Net-(U2-Pad7)" "Net-(U2-Pad8)" "TxD_USB" "RxD_USB" "Net-(U2-Pad15)" "Net-(U2-Pad16)" "Net-(U2-Pad19)"
    "Net-(U2-Pad20)" "Net-(U2-Pad21)" "Net-(U2-Pad22)" "Net-(U2-Pad23)" "Net-(U2-Pad24)" "Net-(U2-Pad25)" "Net-(U2-Pad26)" "Net-(U2-Pad33)"
    "Net-(U2-Pad34)" M0 "Net-(U3-Pad7)" "Net-(U3-Pad8)" "Net-(U3-Pad9)" "Net-(U3-Pad10)" "Net-(U3-Pad11)" "Net-(U3-Pad12)"
    "DTR_USB" "Net-(U3-Pad14)" "Net-(U3-Pad15)" HM1 HM2 "Net-(J12-Pad2)" "Net-(J12-Pad1)" RST
    "RESET_M" "Net-(J2-Pad40)" "Net-(J2-Pad38)" "Net-(J2-Pad37)" "Net-(J2-Pad36)" "Net-(J2-Pad35)" "Net-(J2-Pad33)" "Net-(J2-Pad32)"
    "Net-(J2-Pad28)" "Net-(J2-Pad27)" "Net-(J2-Pad26)" "Net-(J2-Pad24)" "Net-(J2-Pad23)" "Net-(J2-Pad22)" "Net-(J2-Pad21)" "Net-(J2-Pad19)"
    "Net-(J2-Pad18)" "Net-(J2-Pad16)" "Net-(J2-Pad15)" "Net-(J2-Pad13)" "Net-(J2-Pad12)" "Net-(J2-Pad11)" "Net-(J2-Pad7)" "Net-(J2-Pad5)"
    "Net-(J2-Pad3)"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class 5V
    (clearance_class 5V)
    (via_rule 5V)
    (rule
      (width 400.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)